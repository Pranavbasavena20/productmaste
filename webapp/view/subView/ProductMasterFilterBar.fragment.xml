						<suggestionItems>
							<core:Item key="{ProductMasterModel>PRODUCT_DATA/REQUEST_ID}" text="{ProductMasterModel>PRODUCT_DATA/REQUEST_ID}"/>
						</suggestionItems>
					</Input>
				</filterbar:control>
