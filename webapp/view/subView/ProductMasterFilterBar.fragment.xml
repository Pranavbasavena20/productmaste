			<filterbar:FilterGroupItem groupName="G1" name="TemplateName" groupTitle="{i18n>LblFILTER}" label="{i18n>Brand}" labelTooltip="{i18n>Brand}"
				visibleInFilterBar="true">
				<filterbar:control>
					<!--<Input placeholder="Select Brand" showSuggestion="true" showValueHelp="true" valueHelpRequest=".onValueHelpRequest"-->
					<!--	suggestionItems="{/ProductCollection}">-->
					<!--	<suggestionItems>-->
